<!DOCTYPE html>
<html>

<%- include('head'); %>

<body>
  <%- include('navbar', { user: currentUser }); %>

  <p>user: <%= user.username %></p>

  <% if (currentUser) { %>
  <p>currentUser: <%= currentUser.username %> </p>
  <img src="/<%= currentUser.image %>" class="img-fluid" style="height: 13rem; object-fit: cover;" alt="user">
  <% } %>

  <%- include('scripts', {page: 'index'}); %>

</body>

</html>